#include<bits/stdc++.h>
using namespace std;
#define		pb              push_back
#define		PB              pop_back
#define		nn              "\n"
#define		all(p)          p.begin(),p.end()
#define		zz(v)           v.size()
#define		ss              ' '
#define		gcd(a,b)        __gcd(a,b)
#define		lcm(a,b)        (a*b)/gcd(a,b)
#define		pi              acos(-1.0)
typedef		long long       ll;
typedef		vector<ll>      vll;

int main()
{
    ll a,b,c=0,i,j,t,k,m,n,o,x,y,z;
    string s;
    cin>>t;
    while(t--)
    {
        cin>>s;
        if(zz(s)==5) cout<<3<<nn;
        else{
            if((s[0]=='o'&&s[1]=='n') || ( s[0]=='o' && s[2]=='e') || (s[1]=='n' && s[2]=='e')) cout<<1<<nn;
            else cout<<2<<nn;
        }
    }

    return 0;
}
